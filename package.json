{
  "name": "vitalis-backend",
  "version": "1.0.0",
  "description": "Backend API for VITALIS Clinic Management System",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "migrate:sync": "node src/scripts/migrate-sync.js",
    "create-admin": "node src/scripts/create-admin.js",
    "show-admin": "node src/scripts/show-admin.js",
    "get-lab-requests": "node src/scripts/get-all-lab-requests.js",
    "fix-lab-payments": "node src/scripts/fix-lab-requests-payments.js",
    "check-lab-status": "node src/scripts/check-lab-request-status.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "clinic",
    "medical",
    "management",
    "api"
  ],
  "author": "VITALIS Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "puppeteer": "^21.6.1",
    "handlebars": "^4.7.8",
    "exceljs": "^4.4.0",
    "uuid": "^9.0.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
